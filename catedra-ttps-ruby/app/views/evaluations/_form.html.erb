<%= simple_form_for(@evaluation) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :approbation_grade %>
    <%= f.input :date %>
    <%= f.association :course %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
